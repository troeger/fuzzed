{
    "kind": "dfd",
    "name": "Data Flow Diagram",
    "edges": {
        "properties": {
            "name": {
                "kind": "text",
                "default": "",
                "displayName": "Name",

                "label": {
                    "style": []
                }
            }
        }
    },
    "nodes": {
        "node": {
            "nodeDisplayName": "Node",
            "numberOfIncomingConnections": -1,
            "numberOfOutgoingConnections": -1,

            "allowConnectionTo": [],
            "copyable": true,
            "deletable": true,

            "connector": {
                "offset": {
                    "top":    0,
                    "bottom": 0
                }
            },

            "properties": {
                "name" : {
                    "kind": "text",
                    "default": "Node",
                    "displayName": "Name",

                    "mirror": {
                        "position": "bottom",
                        "style": ["bold", "large"]
                    }
                }
            }
        },

        "process": {
            "nodeDisplayName": "Process",
            "help": "This is a process! weeh",
            "inherits": "node",
            "image": "process.svg",

            "allowConnectionTo": ["node"],

            "properties": {
                "name": {
                    "default": "Process"
                }
            }
        },

        "storage": {
            "nodeDisplayName": "Storage",
            "help": "A Storage yoh",
            "inherits": "node",
            "image": "storage.svg",

            "allowConnectionTo": ["process"],

            "properties": {
                "name": {
                    "default": "Storage"
                }
            }
        },

        "external": {
            "nodeDisplayName": "External Entity",
            "help": "EE",
            "inherits": "node",
            "image": "external.svg",

            "allowConnectionTo": ["process"],

            "properties": {
                "name": {
                    "default": "External Entity"
                }
            }
        },
        
        "stickyNote": {
            "nodeDisplayName": "Sticky Note",
            "inherits": "node",
            "numberOfOutgoingConnections": 0,
            "numberOfIncomingConnections": 0,
            "image": "sticky_note.html",
            "resizable": true,
            "editable": true,
            
            "properties": {
              "name" : {
                  "hidden": true
              },
        
              "description": {
                  "kind": "textfield",
                  "default": "Sample Text",
                  "hidden": true
              },
              
              "height": {
                  "kind": "numeric",
                  "step": 1,
                  "default": 150,
                  "hidden": true
              },
              
              "width": {
                  "kind": "numeric",
                  "step": 1,
                  "default": 150,
                  "hidden": true
              }              
            }
        }
    },
    "nodeGroups": {
        "properties": {
            "name" : {
                "kind": "text",
                "default": "Node Group",
                "displayName": "Name",

                "mirror": {
                    "position": "top",
                    "style": [ "italic", "large" ]
                }
            }
        }
    },


    "propertiesDisplayOrder": [
        "name",
        "description",
        "width",
        "height"
    ],

    "shapeMenuNodeDisplayOrder": [
        "process",
        "storage",
        "external",
        "stickyNote"
    ]
}